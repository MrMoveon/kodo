{
  "name": "kodo",
  "version": "0.1.0",
  "description": "react UI library",
  "main": "./src/index.js",
  "scripts": {
    "test": "node ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --colors ./test/**/*.spec.js",
    "dev-server": "gulp server",
    "dev": "webpack --watch --colors --config ./bin/webpack.config.js",
    "product": "NODE_ENV=production webpack --colors --config ./bin/webpack.config.js --optimize-minimize",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "examp": "NODE_ENV=EXAMPDEV webpack --watch --colors --config ./examples/bin/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mulgore/kodo.git"
  },
  "keywords": [
    "react",
    "ui",
    "reactui",
    "ui"
  ],
  "author": "icepy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mulgore/kodo/issues"
  },
  "homepage": "https://github.com/mulgore/kodo#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.0.2",
    "react-router": "^2.5.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.13.0",
    "coveralls": "^2.11.9",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.5",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "koa": "^1.2.0",
    "koa-logger": "^1.3.0",
    "koa-static": "^2.0.0",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.0"
  }
}
